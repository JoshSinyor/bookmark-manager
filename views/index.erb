<div class = 'toolbar'>
  <form action = '/add_bookmark'>
    <input type = 'submit' value = 'Add link'>
  </form>
</div>

<div>
  <ul>
    <% @bookmarks.each do |bookmark| %>
    <% bookmark.each do |title, url| %>
      <li><a href="<%=url%>"><%=title%></a></li>
      <% end %>
      <!-- <li> <a href=<%=bookmark[:url]%>> <%=bookmark['title']%> </a></li> -->
    <% end %>
  </ul>
</div>
